{
  "name": "nstwf/mysql-connection-pool",
  "description": "Reactphp MySQL connection pool",
  "license": [
    "MIT"
  ],
  "authors": [
    {
      "name": "Timur Sadullayev",
      "email": "nstwf.pro@gmail.com"
    }
  ],
  "require": {
    "php": "^8.1",
    "nstwf/mysql-connection": "^1.2"
  },
  "config": {
    "platform": {
      "php": "8.1.0"
    },
    "classmap-authoritative": true,
    "optimize-autoloader": true,
    "sort-packages": true
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "react/async": "^4.0"
  },
  "autoload": {
    "psr-4": {
      "Nstwf\\MysqlConnectionPool\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Nstwf\\MysqlConnectionPool\\": "tests/src"
    }
  },
  "version": "1.2.0"
}